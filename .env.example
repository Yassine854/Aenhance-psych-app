APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

# Jitsi (video calls)
JITSI_DOMAIN=8x8.vc

# 8x8 Jitsi as a Service (JaaS)
# Your App ID (you provided): vpaas-magic-cookie-f1e4f97eb8a84aefbac05880cf3c5e36
JAAS_APP_ID=vpaas-magic-cookie-f1e4f97eb8a84aefbac05880cf3c5e36

# Full Key ID header format: vpaas-magic-cookie-<APP_ID>/<KEY_ID>
JAAS_KID=

# RSA private key PEM (paste with \n for newlines)
JAAS_PRIVATE_KEY=

# Alternatively (recommended on Windows): store the PEM in a file and point to it.
# Example: C:\\Users\\you\\Desktop\\psych-app\\storage\\app\\jaas-private-key.pem
JAAS_PRIVATE_KEY_PATH=

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

BROADCAST_DRIVER=log
CACHE_DRIVER=file
FILESYSTEM_DISK=local
